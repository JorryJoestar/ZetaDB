@startuml

class dataPage {
    -mark bool
    -modified bool
    -pageId uint32
    -tableId uint32
    -priorPageId uint32
    -nextPageId uint32
    -tupleNum int32
    -tuples []*Tuple

    +NewDataPageFromBytes(bytes []byte, schema *Schema) (*dataPage, error)
    +NewDataPage(pageid uint32, tableid uint32, priorPageid uint32, nextPageid uint32) *dataPage
    +MarkDataPage()
    +UnmarkDataPage()
    +ModifyDataPage()
    +UnmodifyDataPage()
    +DataPageToBytes() []byte
    +DpSizeInByte() int
    +DpVacantByteNum() int
